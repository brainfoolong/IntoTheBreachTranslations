msgctxt "scripts/text.lua_Order_Emerge"
msgid "Enemies Emerge"
msgstr "Ennemis Émergent"

msgctxt "scripts/text.lua_Order_Env"
msgid "Environment"
msgstr "Environnement"

msgctxt "scripts/text.lua_Order_Smoke"
msgid "Storm Smoke"
msgstr "Tempête de Fumée"

msgctxt "scripts/text.lua_Deploying_Mech"
msgid "Deploying $1"
msgstr "Déploiement $1"

msgctxt "scripts/text.lua_Deploying_Instructions"
msgid "Select a location in the yellow Drop Zone"
msgstr "Sélectionner un emplacement dans la Zone de Largage jaune"

msgctxt "scripts/text.lua_Deploying_Complete"
msgid "Deployment Complete"
msgstr "Déploiement Terminé"

msgctxt "scripts/text.lua_Deploying_Confirm"
msgid "You may change your placements before confirming"
msgstr "Vous pouvez changer vos emplacements avant de confirmer"

msgctxt "scripts/text.lua_Combat_Deployment"
msgid "DEPLOYMENT"
msgstr "DÉPLOIEMENT"

msgctxt "scripts/text.lua_Deploy_Remaining"
msgid "Remaining Units:"
msgstr "Unités Restantes :"

msgctxt "scripts/text.lua_Button_Deployment_Done"
msgid "CONFIRM"
msgstr "CONFIRMER"

msgctxt "scripts/text.lua_Button_Button_Pawnlist"
msgid "     Unit List"
msgstr "     Liste des Unités"

msgctxt "scripts/text.lua_Button_Button_Objectives"
msgid "Bonus Objectives     "
msgstr "Objectifs Bonus     "

msgctxt "scripts/text.lua_Skip_Units"
msgid "You still have units that can act. They will lose their turn."
msgstr ""
"Vous avez toujours des unités qui peuvent agir. Elles vont perdre leur tour."

msgctxt "scripts/text.lua_Reset_Turn"
msgid ""
"Go back in time to the start of your turn?\n"
"\n"
"Power limitations means you can only do this once per battle."
msgstr ""
"Revenir dans le temps au début de votre tour ?\n"
"\n"
"Les limitations de puissance signifient que vous ne pouvez le faire qu'une "
"fois par bataille."

msgctxt "scripts/text.lua_Reset_TurnExtra"
msgid ""
"Go back in time to the start of your turn?\n"
"\n"
"Your 'Temporal Reset' Pilot means you can do this twice this battle."
msgstr ""
"Revenir dans le temps au début de votre tour ?\n"
"\n"
"La 'Réinitialisation Temporelle' du Pilote signifie que vous pouvez le faire "
"deux fois durant cette bataille."

msgctxt "scripts/text.lua_Reset_TurnFinal"
msgid ""
"Go back in time to the start of your turn?\n"
"\n"
"You only have one Reset remaining."
msgstr ""
"Revenir dans le temps au début de votre tour ?\n"
"\n"
"Il ne vous reste plus qu'une Réinitialisation."

msgctxt "scripts/text.lua_Button_Undo"
msgid ""
"UNDO \n"
"MOVE"
msgstr ""
"ANNULER \n"
"DÉPLACEMENT"

msgctxt "scripts/text.lua_Button_UndoTurn"
msgid "RESET TURN"
msgstr "RÉINITIALISER TOUR"

